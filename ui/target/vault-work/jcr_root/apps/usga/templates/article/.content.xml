<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:description="Article page template for authoring new articles."
          jcr:primaryType="cq:Template"
          jcr:title="Article Page"
          allowedPaths="[/content/usga(/.*)?]"
          ranking="{Long}100">
    <jcr:content
            cq:designPath="/etc/designs/usga"
            cq:template="/apps/usga/templates/articlepage"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="usga/pages/articlePage">
        <contentPar1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/parsys">
            <articleheader
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="usga/components/article/articleHeader"/>
            <articlesplash
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="usga/components/article/articleSplash">
            </articlesplash>
        </contentPar1>
        <contentPar2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/parsys">
            <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="usga/components/text"/>
        </contentPar2>
    </jcr:content>
</jcr:root>
