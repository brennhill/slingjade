<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:description="Video page for displaying brightcove videos"
          jcr:primaryType="cq:Template"
          jcr:title="Video Page"
          allowedPaths="[/content/usga(/.*)?,/content/clubhouse(/.*)?]"
          ranking="{Long}100">
    <jcr:content
            cq:designPath="/etc/designs/usga"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="/apps/usga/pages/videoPage">
        <videoPlayer jcr:primaryType="nt:unstructured"
                     sling:resourceType="usga/components/video/videoPlayer"/>
        <relatedVideos jcr:primaryType="nt:unstructured"
                     sling:resourceType="usga/components/video/relatedVideos"/>
    </jcr:content>
</jcr:root>
