<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          height="400"
          stateful="false"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <basic
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/pages/pageBase/tab_basic.infinity.json"
                        xtype="cqinclude"/>
                <advanced
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/pages/pageBase/tab_advanced.infinity.json"
                        xtype="cqinclude"/>
                <image
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/pages/pageBase/tab_image.infinity.json"
                        xtype="cqinclude"/>
                <cloudservices
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/pages/pageBase/tab_cloudservices.infinity.json"
                        xtype="cqinclude"/>
                <blueprint
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/pages/pageBase/tab_blueprint.infinity.json"
                        xtype="cqinclude"/>
                <livecopy
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/pages/pageBase/tab_livecopy.infinity.json"
                        xtype="cqinclude"/>
                <publishDate
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/pages/contentFeedBasePage/publish-date-properties-dialog.infinity.json"
                        xtype="cqinclude"/>
                <usga_settings
                        jcr:primaryType="cq:Panel"
                        title="USGA Settings">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <nav_mode
                                jcr:primaryType="cq:Widget"
                                collapsed="{Boolean}false"
                                collapsible="{Boolean}true"
                                title="Navigation mode"
                                xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <isNotOverlap
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Navigation not overlaps content: "
                                        name="./isNotOverlap"
                                        type="checkbox"
                                        xtype="selection"/>
                            </items>
                        </nav_mode>
                        <search_config
                                jcr:primaryType="cq:Widget"
                                collapsed="{Boolean}false"
                                collapsible="{Boolean}true"
                                title="Search configuration"
                                xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <doNotShowInLatest
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Do not show in latest: "
                                        name="./doNotShowInLatest"
                                        type="checkbox"
                                        xtype="selection"/>
                            </items>
                        </search_config>
                    </items>
                </usga_settings>
            </items>
        </tabs>
    </items>
</jcr:root>
