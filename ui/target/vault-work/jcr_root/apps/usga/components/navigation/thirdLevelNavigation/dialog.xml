<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          width="{Long}660"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab
                jcr:primaryType="cq:Widget"
                hideMode="offsets"
                xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <multifield
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./subNavLinks"
                        hideLabel="false"
                        fieldLabel="Sub Navigation Links"
                        name="./subNavLinks"
                        xtype="custommultifield">
                    <fieldConfig jcr:primaryType="cq:WidgetCollection">
                        <linkLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Link Label"
                                name="./linkLabel"
                                xtype="textfield"/>
                        <linkURL
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Link Destination"
                                name="./linkURL"
                                xtype="pathfield"/>
                        <newWindow
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Open link in a new window"
                                defaultValue="no"
                                boxMaxWidth="{Long}200"
                                name="./newWindow"
                                type="select"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <yes
                                        jcr:primaryType="nt:unstructured"
                                        text="Yes"
                                        value="yes"/>
                                <no
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="no"/>
                            </options>
                        </newWindow>
                        <membershipType
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Type for Membership: "
                                    boxMaxWidth="{Long}200"
                                    defaultValue=""
                                    name="./membershipType"
                                    type="select"
                                    xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <none
                                            jcr:primaryType="nt:unstructured"
                                            text="none"
                                            value=""/>
                                    <join
                                            jcr:primaryType="nt:unstructured"
                                            text="Join"
                                            value="default"/>
                                    <renew
                                            jcr:primaryType="nt:unstructured"
                                            text="Renew membership"
                                            value="default expired-federated expiring-federated-within-renewal-window"/>
                                    <reacquire
                                            jcr:primaryType="nt:unstructured"
                                            text="Reacquire membership"
                                            value="lapsed-federated"/>
                                    <gift
                                            jcr:primaryType="nt:unstructured"
                                            text="Give a Membership Gift"
                                            value="default expired-federated expiring-federated-within-renewal-window lapsed-federated federated-outside-renewal-window"/>
                                </options>
                         </membershipType>
                    </fieldConfig>
                </multifield>
            </items>
        </tab>
    </items>
</jcr:root>