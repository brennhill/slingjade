<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          height="800"
          width="750"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab1
                jcr:primaryType="cq:Widget"
                xtype="tabpanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <options1
                        jcr:primaryType="cq:Widget"
                        title="Feed Driven configuration"
                        xtype="panel">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <channels jcr:primaryType="cq:Widget"
                                  title="Channels"
                                  xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <everything
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Content feed for all types: "
                                        name="./everything"
                                        xtype="textfield"/>
                                <news
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Content feed for articles: "
                                        name="./news"
                                        xtype="textfield"/>
                                <videos
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Content feed for videos: "
                                        name="./videos"
                                        xtype="textfield"/>
                                <photos
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Content feed for photos: "
                                        name="./photos"
                                        xtype="textfield"/>
                                <social
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Content feed for social: "
                                        name="./social"
                                        xtype="textfield"/>
                            </items>
                        </channels>
                        <defaultChannel jcr:primaryType="cq:Widget"
                                        fieldLabel="Default Channel: "
                                        boxMaxWidth="{Long}300"
                                        defaultValue="everything"
                                        name="./defaultChannel"
                                        type="select"
                                        xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <everything jcr:primaryType="nt:unstructured"
                                            text="everything"
                                            value="everything"/>
                                <news jcr:primaryType="nt:unstructured"
                                      text="news"
                                      value="news"/>
                                <videos jcr:primaryType="nt:unstructured"
                                        text="videos"
                                        value="videos"/>
                                <photos jcr:primaryType="nt:unstructured"
                                        text="photos"
                                        value="photos"/>
                                <social jcr:primaryType="nt:unstructured"
                                        text="social"
                                        value="social"/>
                            </options>
                        </defaultChannel>
                        <hubmode
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Hub mode"
                                name="./hubmode"
                                type="radiogroup"
                                defaultValue="false"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <short
                                        jcr:primaryType="nt:unstructured"
                                        text="Non-hub mode"
                                        value="false"
                                        xtype="radio"/>
                                <long
                                        jcr:primaryType="nt:unstructured"
                                        checked="true"
                                        text="Hubmode"
                                        value="true"
                                        xtype="radio"/>
                            </options>
                        </hubmode>
                      <hubtype
                          jcr:primaryType="cq:Widget"
                          fieldLabel="Hub type"
                          name="./hubtype"
                          type="radiogroup"
                          defaultValue="long"
                          xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                          <long
                              jcr:primaryType="nt:unstructured"
                              checked="true"
                              text="Long"
                              value="long"
                              xtype="radio"/>
                          <short
                              jcr:primaryType="nt:unstructured"
                              text="Short"
                              value="short"
                              xtype="radio"/>
                        </options>
                      </hubtype>
                        <tags
                                jcr:primaryType="cq:Widget"
                                cls="cq-propsdialog-tags"
                                fieldLabel="Categories for drop down"
                                name="./categoriesTags"
                                namespaces="[usga-category]"
                                xtype="tags"/>
                        <moreLinkUrl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="More Link Url: "
                                name="./moreLinkUrl"
                                xtype="pathfield"/>
                        <itemsCount jcr:primaryType="cq:Widget"
                                    title="Items Count"
                                    xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <limit
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Limit: "
                                        name="./limit"
                                        xtype="numberfield"/>
                                <adaptItems
                                        jcr:primaryType="cq:Widget"
                                        fieldLabel="Avoid empty space: "
                                        name="./adaptItems"
                                        xtype="selection"
                                        type="checkbox"/>
                            </items>
                        </itemsCount>
                        <hideFilters
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Hide Filters"
                                name="./hideFilters"
                                type="radiogroup"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <none
                                        jcr:primaryType="nt:unstructured"
                                        checked="true"
                                        text="None"
                                        value="none"
                                        xtype="radio"/>
                                <all
                                        jcr:primaryType="nt:unstructured"
                                        text="All"
                                        value="all"
                                        xtype="radio"/>
                                <categoriesFilter
                                        jcr:primaryType="nt:unstructured"
                                        text="Categories Filter"
                                        value="categories"
                                        xtype="radio"/>
                                <channelsFilter
                                        jcr:primaryType="nt:unstructured"
                                        text="Channels Filter"
                                        value="channels"
                                        xtype="radio"/>
                            </options>
                        </hideFilters>
                        <hideShowMoreButton
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Hide Show More Button"
                                name="./hideShowMoreButton"
                                xtype="selection"
                                type="checkbox"/>
                        <publishRange
                            jcr:primaryType="cq:Widget"
                            title="Publish Date: "
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                 <publishedAfter
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="From: "
                                    name="./publishedAfter"
                                    xtype="datetime"/>
                                 <publishedBefore
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="To: "
                                    name="./publishedBefore"
                                    xtype="datetime"/>
                            </items>
                        </publishRange>
                    </items>
                </options1>
                <options2
                        jcr:primaryType="cq:Widget"
                        title="Editorially Driven configuration"
                        xtype="panel">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <box1
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Box 1: "
                                name="./box1"
                                xtype="pathfield"/>
                        <box2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Box 2(The large content box): "
                                name="./box2"
                                xtype="pathfield"/>
                        <box3
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Box 3: "
                                name="./box3"
                                xtype="pathfield"/>
                        <box4
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Box 4: "
                                name="./box4"
                                xtype="pathfield"/>
                        <box5
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Box 5: "
                                name="./box5"
                                xtype="pathfield"/>
                        <box6
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Box 6: "
                                name="./box6"
                                xtype="pathfield"/>
                        <box7
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Box 7: "
                                name="./box7"
                                xtype="pathfield"/>
                      </items>


                </options2>
            </items>
        </tab1>
    </items>
</jcr:root>