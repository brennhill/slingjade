<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Panel"
          title="Override">
    <items jcr:primaryType="cq:WidgetCollection">
        <enableOverride jcr:primaryType="cq:Widget"
                    fieldLabel="Override Enabled"
                    name="./enableOverride"
                    type="checkbox"
                    xtype="selection"/>
        <override
                jcr:primaryType="cq:Widget"
                fieldLabel="Messages"
                fieldDescription="Only one multifield item should be added"
                name="./override/messages"
                xtype="multifield">
            <fieldConfig
                    jcr:primaryType="cq:Widget"
                    xtype="fixedmultifieldpanel">
                <items
                        jcr:primaryType="cq:WidgetCollection"
                        path="/apps/usga/components/clubhouse/welcomeWidget/items.infinity.json"
                        xtype="cqinclude"/>
            </fieldConfig>
        </override>
        <overrideName
                jcr:primaryType="cq:Widget"
                defaultValue= "Override Message"
                hidden="{Boolean}true"
                name="./override/name"
                value= "Override Message"
                xtype="hidden"/>
    </items>
</jcr:root>
