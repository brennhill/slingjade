<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab
                jcr:primaryType="cq:Widget"
                hideMode="offsets"
                xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <lbSponsorIcon
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Leader Board Sponsor Icon: "
                        rootPath="/content/dam"
                        name="./lbSponsorIcon"
                        xtype="pathfield"/>
                <lbSponsorLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Leader Board Sponsor Link: "
                        rootPath="/content"
                        name="./lbSponsorLink"
                        xtype="pathfield"/>
                <stSponsorIcon
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Starting Time Sponsor Icon: "
                        rootPath="/content/dam"
                        name="./stSponsorIcon"
                        xtype="pathfield"/>
                <stSponsorLink
                        jcr:primaryType="cq:Widget"
                        rootPath="/content"
                        fieldLabel="Starting Time Sponsor Link: "
                        name="./stSponsorLink"
                        xtype="pathfield"/>
                <championshipPath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Championship Path"
                        editable="false"
                        rootPath="/content/admin/schedule"
                        allowBlank="{Boolean} false"
                        name="./championshipPath"
                        xtype="pathfield"/>
            </items>
        </tab>
    </items>
</jcr:root>