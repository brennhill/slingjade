<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab
                jcr:primaryType="cq:Widget"
                hideMode="offsets"
                xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <championshipType
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/components/leaderboard/leaderboard/championship_type_dialog/main/items/championshipType.infinity.json"
                        xtype="cqinclude"/>
                <selectList
                        jcr:primaryType="cq:Widget"
                        path="/apps/usga/components/leaderboard/leaderboard/championship_type_dialog/main/items/selectList.infinity.json"
                        xtype="cqinclude"/>
                <strokeDataUrl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Stroke data url: "
                        fieldDescription="Do not fill for Cup Event"
                        name="./strokeDataUrl"
                        xtype="textfield"/>
                <playOffUrl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Play Off data url: "
                        name="./playOffUrl"
                        xtype="textfield"/>
                <tabControlUrl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Tab control data url: "
                        name="./tabControlUrl"
                        xtype="textfield"/>
                <matchPlayDataUrls
                        jcr:primaryType="cq:Widget"
                        hideLabel="false"
                        name="./matchPlayDataUrls"
                        width="250"
                        xtype="multifield"
                        fieldLabel="Match Play Data Urls"
                        fieldDescription="This property for Amateur, Fourball and Cup Events Championships only">
                    <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="textfield">
                        <listeners jcr:primaryType="nt:unstructured"/>
                    </fieldConfig>
                </matchPlayDataUrls>
                <bracketDataUrl
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Bracket Data Url: "
                        fieldDescription="This property for Amateur and Fourball Championships only"
                        name="./bracketDataUrl"
                        xtype="textfield"/>
            </items>
        </tab>
    </items>
</jcr:root>