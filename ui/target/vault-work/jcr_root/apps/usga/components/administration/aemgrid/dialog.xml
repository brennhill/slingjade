<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          width="{Long}800"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab
                jcr:primaryType="cq:Widget"
                hideMode="offsets"
                xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <memberType
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Membership Type"
                        name="./membertype"
                        xtype="textfield"/>
                <code
                  jcr:primaryType="cq:Widget"
                  fieldLabel="Code"
                  name="./code"
                  xtype="textfield"/>
                <label
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Code"
                    name="./label"
                    xtype="textfield"/>
                <multifieldSingle
                        jcr:primaryType="cq:Widget"
                        customMultiFieldName="./singlepricing"
                        hideLabel="false"
                        fieldLabel="Single Pricing"
                        name="./singlepricing"
                        xtype="custommultifield">
                    <fieldConfig jcr:primaryType="cq:WidgetCollection">
                      <lastMin
                          jcr:primaryType="cq:Widget"
                          allowBlank="{Boolean} false"
                          fieldLabel="Last Minimum Payment"
                          allowNegative="${Boolean}false"
                          name="./lastMin"
                          xtype="numberfield"/>
                      <lastMax
                          jcr:primaryType="cq:Widget"
                          allowBlank="{Boolean} false"
                          fieldLabel="Last Max Payment"
                          allowNegative="${Boolean}false"
                          name="./lastMax"
                          xtype="numberfield"/>
                      <join
                          jcr:primaryType="cq:Widget"
                          allowBlank="{Boolean} false"
                          fieldLabel="Join Amount"
                          allowNegative="${Boolean}false"
                          name="./joinCost"
                          xtype="numberfield"/>
                      <renew
                          jcr:primaryType="cq:Widget"
                          allowBlank="{Boolean} false"
                          fieldLabel="Renew Amount"
                          allowNegative="${Boolean}false"
                          name="./renewCost"
                          xtype="numberfield"/>
                    </fieldConfig>
                </multifieldSingle>
              <multifieldDual
                  jcr:primaryType="cq:Widget"
                  customMultiFieldName="./dualpricing"
                  hideLabel="false"
                  fieldLabel="Double Pricing"
                  name="./dualpricing"
                  xtype="custommultifield">
                <fieldConfig jcr:primaryType="cq:WidgetCollection">
                  <lastMin
                      jcr:primaryType="cq:Widget"
                      allowBlank="{Boolean} false"
                      fieldLabel="Last Minimum Payment"
                      allowNegative="${Boolean}false"
                      name="./lastMin"
                      xtype="numberfield"/>
                  <lastMax
                      jcr:primaryType="cq:Widget"
                      allowBlank="{Boolean} false"
                      fieldLabel="Last Max Payment"
                      allowNegative="${Boolean}false"
                      name="./lastMax"
                      xtype="numberfield"/>
                  <join
                      jcr:primaryType="cq:Widget"
                      allowBlank="{Boolean} false"
                      fieldLabel="Join Amount"
                      allowNegative="${Boolean}false"
                      name="./joinCost"
                      xtype="numberfield"/>
                  <renew
                      jcr:primaryType="cq:Widget"
                      allowBlank="{Boolean} false"
                      fieldLabel="Renew Amount"
                      allowNegative="${Boolean}false"
                      name="./renewCost"
                      xtype="numberfield"/>
                </fieldConfig>
              </multifieldDual>
            </items>
        </tab>
    </items>
</jcr:root>